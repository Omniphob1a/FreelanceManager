{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ProjectService": {
    "BaseUrl": "http://gateway:8080/api/"
  },
  "ConnectionStrings": {
    "ProjectTasksDbContext": "Server=tasks-db;Port=5435;Database=tasks;User Id=postgres;Password=123;",
    "Redis": "redis:6379"
  },
  "JwtSettings": {
    "Issuer": "MyApi",
    "Audience": "Users",
    "SecretKey": "testKeytestKeytestKey123123123123",
    "ExpiryMinutes": 6000
  },
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "GroupId": "tasks-service",
    "AutoOffsetReset": "Earliest",
    "EnableAutoCommit": false,
    "Options": {
      "EnableIdempotence": true
    }
  },
  "AllowedHosts": "*"
}
